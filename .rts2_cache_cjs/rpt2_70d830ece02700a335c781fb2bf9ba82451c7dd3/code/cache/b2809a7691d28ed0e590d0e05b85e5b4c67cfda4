{"code":"import React, { useEffect, useState, useRef, useContext } from \"react\";\r\nimport { YearSide, TopScroll, BottomScroll } from \"../_shared/style\";\r\nimport Swiper from 'react-id-swiper';\r\nimport 'swiper/swiper-bundle.css';\r\nimport moment from \"jalali-moment\";\r\nimport { ContextApiDate } from \"../utils/ContextApi\";\r\nfunction SideBar() {\r\n    const { value, setValue, setOptionActive } = useContext(ContextApiDate);\r\n    const [data, setData] = useState([]);\r\n    useEffect(() => {\r\n        let prevYears = (getPrevYears(50));\r\n        let nextYears = getNextYears(50).reverse();\r\n        let newYears = nextYears.concat(prevYears);\r\n        setData(newYears);\r\n    }, []);\r\n    function getPrevYears(numOfYears) {\r\n        let nowDateMini = moment(value).subtract(1, 'year');\r\n        return Array.from({ length: numOfYears }, (v, i) => {\r\n            localStorage.setItem(\"laruz7476\", v ? v : 'emran rastadi');\r\n            return moment(nowDateMini).subtract(i, 'year');\r\n        });\r\n    }\r\n    function getNextYears(numOfYears) {\r\n        return Array.from({ length: numOfYears }, (v, i) => {\r\n            localStorage.setItem(\"laruz7476\", v ? v : 'emran rastadi');\r\n            return moment(value).add(i, 'year');\r\n        });\r\n    }\r\n    const swiperRef = useRef(null);\r\n    const params = {\r\n        // direction: \"vertical\",\r\n        slidesPerView: 8,\r\n        spaceBetween: 5,\r\n        grabCursor: true,\r\n        centeredSlides: true,\r\n        freeMode: true,\r\n    };\r\n    return (React.createElement(YearSide, null,\r\n        React.createElement(TopScroll, null),\r\n        React.createElement(React.Fragment, null,\r\n            React.createElement(Swiper, { ref: swiperRef, ...params, activeSlideKey: value ? value.format('YYYY').toString() : '0', direction: 'vertical' }, data.length > 20 ? data.map((item) => {\r\n                let itemYear = moment(item).locale('fa').format('YYYY');\r\n                return (React.createElement(\"div\", { key: moment(item).locale('fa').format('YYYY'), onClick: () => {\r\n                        setValue(item);\r\n                        setOptionActive(false);\r\n                    }, className: value ? value.format('YYYY') === itemYear ? 'active' : '' : '' }, itemYear));\r\n            }) : '')),\r\n        React.createElement(BottomScroll, null)));\r\n}\r\nexport default SideBar;\r\n//# sourceMappingURL=SideBar.js.map","references":["C:/Users/emran/Desktop/modules/react-butty-persian-calendar/node_modules/@types/react/index.d.ts","C:/Users/emran/Desktop/modules/react-butty-persian-calendar/src/Calendar/_shared/style.tsx","C:/Users/emran/Desktop/modules/react-butty-persian-calendar/node_modules/react-id-swiper/lib/index.d.ts","C:/Users/emran/Desktop/modules/react-butty-persian-calendar/node_modules/jalali-moment/jalali-moment.d.ts","C:/Users/emran/Desktop/modules/react-butty-persian-calendar/src/Calendar/utils/ContextApi.tsx"],"map":"{\"version\":3,\"file\":\"SideBar.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../src/Calendar/components/SideBar.tsx\"],\"names\":[],\"mappings\":\"AAAA,OAAO,KAAK,EAAE,EAAC,SAAS,EAAE,QAAQ,EAAE,MAAM,EAAE,UAAU,EAAC,MAAM,OAAO,CAAC;AACrE,OAAO,EAAC,QAAQ,EAAC,SAAS,EAAE,YAAY,EAAC,MAAM,kBAAkB,CAAC;AAClE,OAAO,MAAM,MAAM,iBAAiB,CAAA;AACpC,OAAO,0BAA0B,CAAA;AACjC,OAAO,MAAM,MAAM,eAAe,CAAC;AACnC,OAAO,EAAC,cAAc,EAAC,MAAM,qBAAqB,CAAC;AAGnD,SAAS,OAAO;IACZ,MAAM,EAAC,KAAK,EAAE,QAAQ,EAAG,eAAe,EAAC,GAAG,UAAU,CAAC,cAAc,CAAC,CAAC;IACvE,MAAM,CAAC,IAAI,EAAE,OAAO,CAAC,GAAG,QAAQ,CAAM,EAAE,CAAC,CAAA;IACzC,SAAS,CAAC,GAAG,EAAE;QACX,IAAI,SAAS,GAAG,CAAC,YAAY,CAAC,EAAE,CAAC,CAAC,CAAA;QAClC,IAAI,SAAS,GAAG,YAAY,CAAC,EAAE,CAAC,CAAC,OAAO,EAAE,CAAA;QAC1C,IAAI,QAAQ,GAAG,SAAS,CAAC,MAAM,CAAC,SAAS,CAAC,CAAA;QAC1C,OAAO,CAAC,QAAQ,CAAC,CAAA;IACrB,CAAC,EAAE,EAAE,CAAC,CAAA;IAEN,SAAS,YAAY,CAAC,UAAkB;QACpC,IAAI,WAAW,GAAG,MAAM,CAAC,KAAK,CAAC,CAAC,QAAQ,CAAC,CAAC,EAAE,MAAM,CAAC,CAAC;QACpD,OAAO,KAAK,CAAC,IAAI,CAAC,EAAC,MAAM,EAAE,